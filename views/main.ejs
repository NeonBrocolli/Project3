        <% if (user) { %>
          <li><a href="/logout" class="white-text"><i class="material-icons left white-text text-darken-3">trending_flat</i>Logout</a></li>
        <% } else { %>
          <li><a href="/auth/google" class="white-text"><i class="material-icons left white-text text-darken-3">vpn_key</i>Login</a></li>
        <% } %>
      </ul>
      <ul class="side-nav" id="mobile">
        <% venues.forEach(function(venue){ %>
        <li><a href="/venues/<%=venue._id%>/places"><%= venue.name %></a></li>
