          <li><a href="/auth/google" class="white-text"><i class="material-icons left white-text text-darken-3">vpn_key</i>Login</a></li>
        <% } %>
      </ul>
      <ul class="side-nav" id="mobile">
        <% venues.forEach(function(venue){ %>
